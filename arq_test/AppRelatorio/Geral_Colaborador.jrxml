	<queryString>
		<![CDATA[select p.pergunta as Pergunta,
(select count(*) from pesquisa where pergunta=p.pergunta and resposta=1) as '1 - Excelente',
(select count(*) from pesquisa where pergunta=p.pergunta and resposta=2) as '2 - Muito Bom',
(select count(*) from pesquisa where pergunta=p.pergunta and resposta=3) as '3 - Bom',
(select count(*) from pesquisa where pergunta=p.pergunta and resposta=4) as '4 - Regular',
(select count(*) from pesquisa where pergunta=p.pergunta and resposta=5) as '5 - Ruim',
(select count(*) from pesquisa where pergunta=p.pergunta and (resposta=1 or resposta=2 or resposta=3 or resposta=4 or resposta=5)) as 'TOTAL'
from pesquisa as p where colaborador=$P{colaborador}
group by pergunta;]]>
	</queryString>
