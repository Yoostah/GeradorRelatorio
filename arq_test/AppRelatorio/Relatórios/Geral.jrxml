	<queryString>
		<![CDATA[select p.colaborador, p.pergunta as Pergunta,
(select count(resposta) from pesquisa where data between str_to_date($P{data_inicial}, "%d/%m/%Y") and str_to_date($P{data_final}, "%d/%m/%Y") and colaborador = p.colaborador and pergunta=p.pergunta and resposta=1) as '1 - Excelente',
(select count(resposta) from pesquisa where data between str_to_date($P{data_inicial}, "%d/%m/%Y") and str_to_date($P{data_final}, "%d/%m/%Y") and colaborador = p.colaborador and pergunta=p.pergunta and resposta=2) as '2 - Muito Bom',
(select count(resposta) from pesquisa where data between str_to_date($P{data_inicial}, "%d/%m/%Y") and str_to_date($P{data_final}, "%d/%m/%Y") and colaborador = p.colaborador and pergunta=p.pergunta and resposta=3) as '3 - Bom',
(select count(resposta) from pesquisa where data between str_to_date($P{data_inicial}, "%d/%m/%Y") and str_to_date($P{data_final}, "%d/%m/%Y") and colaborador = p.colaborador and pergunta=p.pergunta and resposta=4) as '4 - Regular',
(select count(resposta) from pesquisa where data between str_to_date($P{data_inicial}, "%d/%m/%Y") and str_to_date($P{data_final}, "%d/%m/%Y") and colaborador = p.colaborador and pergunta=p.pergunta and resposta=5) as '5 - Ruim',
(select count(resposta) from pesquisa where pergunta=p.pergunta and colaborador = p.colaborador and data between str_to_date($P{data_inicial}, "%d/%m/%Y") and str_to_date($P{data_final}, "%d/%m/%Y")
and (resposta=1 or resposta=2 or resposta=3 or resposta=4 or resposta=5)) as 'TOTAL'
from pesquisa as p where data between str_to_date($P{data_inicial}, "%d/%m/%Y") and str_to_date($P{data_final}, "%d/%m/%Y") and resposta < 6
group by p.colaborador, p.pergunta]]>
	</queryString>
